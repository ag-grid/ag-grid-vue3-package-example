{
  "name": "ag-grid-vue3-package-example",
  "version": "30.0.2",
  "private": true,
  "scripts": {
    "start": "npm run serve",
    "serve": "NODE_OPTIONS=--openssl-legacy-provider vue-cli-service serve --host 0.0.0.0",
    "package": "NODE_OPTIONS=--openssl-legacy-provider vue-cli-service build && npm run hash",
    "hash": "sh ./hashDirectory.sh > .hash",
    "test:tbd": "vue-cli-service test:unit --env=jest-environment-jsdom-fourteen",
    "test:debug": "node --inspect-brk ./node_modules/@vue/cli-service/bin/vue-cli-service.js test:unit"
  },
  "dependencies": {
    "@vue/babel-preset-app": "4.5.13",
    "ag-grid-community": "~30.0.2",
    "ag-grid-enterprise": "~30.0.2",
    "ag-grid-vue3": "~30.0.2",
    "bootstrap": "4.5.0",
    "core-js": "3.11.1",
    "vue": "3.2.33",
    "vue-router": "4.0.15"
  },
  "devDependencies": {
    "@vue/babel-preset-app": "5.0.4",
    "@vue/cli-plugin-babel": "5.0.4",
    "@vue/cli-plugin-eslint": "5.0.4",
    "@vue/cli-plugin-unit-jest": "5.0.4",
    "@vue/cli-service": "5.0.4",
    "@vue/compiler-sfc": "3.2.33",
    "babel-eslint": " 10.1.0",
    "eslint": "7.32.0",
    "eslint-plugin-vue": "7.20.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie >= 0",
    "not ie_mob >= 0"
  ],
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  }
}